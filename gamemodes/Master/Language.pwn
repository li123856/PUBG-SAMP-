stock ReturnMLStr(Laguage,Type)
{
	new line[512];
    switch(Laguage)
    {
        case 0:format(line,sizeof(line),"%s",MultiLanguage[Type][_cn]);
		case 1:format(line,sizeof(line),"%s",MultiLanguage[Type][_en]);
    }
	return line;
}
